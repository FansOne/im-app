.popupBox {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  width: 622rpx;
  height: 430rpx;
  background-color: #fff;
  z-index: 11;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  padding: 53rpx 83rpx;
}
.popupBox image {
  width: 160rpx;
  height: 114rpx;
}
.popupBox .text view {
  font-size: 22rpx;
  width: 100%;
  text-align: center;
}
.popupBox .jumpBtn {
  border-radius: 0;
  font-size: 30rpx;
  width: 100%;
  height: 80rpx;
  background-color: #00D4D7;
  color: #fff;
  font-weight: 600;
  text-align: center;
  line-height: 80rpx;
}
